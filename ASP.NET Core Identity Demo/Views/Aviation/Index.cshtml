@using ASP.NET_Core_Identity_Demo.Models.Aviation
@model Root

<h1>@Model.pagination</h1>

@foreach(var item in Model.data)
{
    <h1>@item.airline</h1>
    <h1>@item.aircraft</h1>
    <h1>@item.arrival</h1>
    <h1>@item.departure</h1>
    <h1>@item.flight</h1>
    <h1>@item.flight_date</h1>
    <h1>@item.flight_status</h1>
    <h1>@item.live</h1>
}